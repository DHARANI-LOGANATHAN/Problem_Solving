Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        // int b=sc.nextInt();
        int factor=0;
        for(int i=1;i<a;i++){
            if(a%i==0){
                // l=i;
                factor=factor+i;
            }
            
        }
        if(factor==a){
            System.out.println("Perfect Number");
        }
        else{
            System.out.println("Not a Perfect Number");
        }
